            "method": "GET",
            "url": "{{baseUrl}}/api/v1/users/{{userId}}"
          }
        },
        {
          "name": "Create User",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"phone\": \"1234567890\",\n  \"address\": \"123 Main St\",\n  \"gender\": \"MALE\",\n  \"type\": \"CUSTOMER\"\n}"
            },
            "url": "{{baseUrl}}/api/v1/users"
          }
        },
        {
          "name": "Update User",
          "request": {
            "method": "PUT",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Updated\",\n  \"email\": \"john.updated@example.com\",\n  \"phone\": \"9876543210\"\n}"
            },
            "url": "{{baseUrl}}/api/v1/users/{{userId}}"
          }
